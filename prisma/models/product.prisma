model Product {
    id            String   @id @default(auto()) @map("_id") @db.ObjectId
    sku           String   @unique
    title         String
    price         Float?
    image         String?
    category      String
    subCategory   String
    brand         String
    createdAt     DateTime @default(now())
    updatedAt     DateTime @updatedAt
    lastCheckedAt DateTime @default(now())

    competitorProducts CompetitorProduct[]
}
